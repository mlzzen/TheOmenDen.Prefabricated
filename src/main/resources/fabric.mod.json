{
  "schemaVersion": 1,
  "id": "prefabricated",
  "version": "${version}",
  "name": "Prefabricated",
  "description": "Prefabricated is a mod which contains multiple pre-designed building for you to place in the world",
  "authors": [
    "WuestMan",
    "Kaojinn",
    "NerdySpider",
    "Dqu1J"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/prefab-fabric",
    "sources": "https://github.com/Brian-Wuest/MC-Prefab-Fabric"
  },
  "license": "MIT",
  "icon": "assets/prefabricated/prefabthumb.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.theomenden.prefabricated.Prefab"
    ],
    "client": [
      "com.theomenden.prefabricated.PrefabClient"
    ],
    "modmenu": [
      "com.theomenden.prefabricated.config.PrefabModMenuIntegration"
    ]
  },
  "accessWidener": "prefabricated.accesswidener",
  "mixins": [
    "prefabricated.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.3",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
  }
}
